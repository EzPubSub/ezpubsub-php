{
    "name": "ezpubsub/ezpubsub-php",
    "description": "PHP library for EzPubSub",
    "keywords": [
        "ezpubsub",
        "ezpubsub-php"
    ],
    "homepage": "https://github.com/ezpubsub/ezpubsub-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Nyi Nyi Lwin",
            "email": "nyinyilwin1992@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "EzPubSub\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EzPubSub\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
